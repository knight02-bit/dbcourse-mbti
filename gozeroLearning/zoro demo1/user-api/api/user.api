//api语法示例及语法说明


// api语法版本
syntax = "v1"

info(
    author: "knight"
    date:   "2022-05-28"
    desc:   "api语法示例及语法说明"
)

type (
	UserInfoReq {
		UserId      int64 `json:"userId"`
	}
	UserInfoResp {
		UserId      int64  `json:"userId"`
		Nickname    string  `json:"nickname"`
	}
)

service user-api{
    @doc "获取用户信息"
    @handler userInfo
    post /user/info (UserInfoReq) returns (UserInfoResp)
}